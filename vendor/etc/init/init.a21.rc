on boot
# Performance
    chown root root /sys/devices/system/cpu/cpufreq/schedutil/down_rate_limit_us
    chown root root /sys/devices/system/cpu/cpufreq/schedutil/up_rate_limit_us
    chmod 0644 /sys/devices/system/cpu/cpufreq/schedutil/down_rate_limit_us
    chmod 0644 /sys/devices/system/cpu/cpufreq/schedutil/up_rate_limit_us
    chown root root /sys/kernel/gpu/gpu_freq_table
    chown root system /sys/kernel/gpu/gpu_max_clock
    chown root system /sys/kernel/gpu/gpu_min_clock
    chmod 0664 /sys/devices/system/cpu/cpufreq/policy0/scaling_min_freq
    chmod 0664 /sys/devices/system/cpu/cpufreq/policy4/scaling_min_freq
    chmod 0664 /sys/devices/system/cpu/cpufreq/policy4/scaling_max_freq
    chmod 0644 /dev/stune/background/schedtune.uclamp.min
    chown root root /dev/stune/background/schedtune.uclamp.min
    chmod 0644 /dev/stune/foreground/schedtune.uclamp.min
    chown root root /dev/stune/foreground/schedtune.uclamp.min
    chmod 0644 /dev/stune/top-app/schedtune.uclamp.min
    chown root root /dev/stune/top-app/schedtune.uclamp.min
    write /dev/cpuset/system-background/cpus 4-7

# SSWAP
    write /proc/sys/vm/swappiness 160

# Create carrier folder for HiddenMenu
on post-fs
    mkdir /efs/carrier 0755 radio system
    chown radio system /efs/carrier
    chown radio system /efs/carrier/HiddenMenu
